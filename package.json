    "concurrently": "^7.1.0",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^27.5.1",
    "jest-image-snapshot": "^5.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.5",
