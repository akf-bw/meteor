    "@storybook/test-runner": "^0.1.1",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/vue": "^6.5.8",
    "@types/jest": "^24.0.19",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
